# ============================================
# SELLO FRONTEND - ENVIRONMENT VARIABLES
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# Note: Vite requires VITE_ prefix for all variables

# ============================================
# API CONFIGURATION
# ============================================
# Backend API URL (include /api if your backend serves from /api)
VITE_API_URL=http://localhost:4000/api

# ============================================
# GOOGLE OAUTH
# ============================================
# Google OAuth Client ID (same as backend GOOGLE_CLIENT_ID)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# ============================================
# GOOGLE MAPS API
# ============================================
# Google Maps API Key for location features
VITE_REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# ============================================
# OPEN ROUTE SERVICE API (Optional)
# ============================================
# For routing and distance calculations
VITE_ORS_API_KEY=your-ors-api-key

# ============================================
# FRONTEND URL (Optional)
# ============================================
# Used for generating invite links, etc.
VITE_FRONTEND_URL=http://localhost:5173


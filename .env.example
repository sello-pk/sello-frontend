# ============================================
# SELLO FRONTEND - ENVIRONMENT VARIABLES
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# Note: Vite requires VITE_ prefix for all variables

# ============================================
# API CONFIGURATION
# ============================================
# Backend API URL (include /api if your backend serves from /api)
VITE_API_URL=http://localhost:4000/api

# ============================================
# GOOGLE OAUTH - PUBLIC CLIENT ID ONLY
# ============================================
# This is safe to expose as it's a public client ID
# Never expose client secrets on the frontend
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# ============================================
# GOOGLE MAPS API - RESTRICTED KEY RECOMMENDED
# ============================================
# Use a restricted API key with HTTP referrer restrictions
# Consider loading this dynamically from server in production
VITE_REACT_APP_GOOGLE_MAPS_API_KEY=your-restricted-google-maps-api-key
VITE_GOOGLE_MAPS_API_KEY=your-restricted-google-maps-api-key

# ============================================
# THIRD-PARTY APIS - CONSIDER SERVER PROXY
# ============================================
# For production, consider proxying these through your backend
# to avoid exposing API keys on the client side
VITE_ORS_API_KEY=your-ors-api-key

# ============================================
# FRONTEND URL (Optional)
# ============================================
# Used for generating invite links, etc.
VITE_FRONTEND_URL=http://localhost:5173

